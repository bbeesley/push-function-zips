{
  "name": "@beesley/push-function-zips",
  "version": "2.2.1",
  "description": "Zips up and uploads build artifacts for serverless functions",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/package-and-upload.d.ts",
      "import": "./dist/esm/package-and-upload.js",
      "require": "./dist/cjs/package-and-upload.cjs"
    }
  },
  "module": "./dist/esm/package-and-upload.js",
  "main": "./dist/cjs/package-and-upload.cjs",
  "types": "./dist/types/package-and-upload.d.ts",
  "bin": {
    "s3upload": "bin/upload.cjs"
  },
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "scripts": {
    "test": "ava",
    "pretest": "npm run lint && npm run compile",
    "lint": "xo",
    "precompile": "rm -rf dist",
    "compile:esm": "tsc -p tsconfig.json",
    "compile:cjs": "babel src --out-dir dist/cjs --extensions '.ts,.cjs,.mjs' --ignore '**/*.test.ts' --source-maps --out-file-extension '.cjs'",
    "compile": "run-p compile:*",
    "documentation": "npx documentation readme src/package-and-upload.ts src/@types/options.ts --github  --require-extension=ts --parse-extension=ts --section=api --shallow",
    "prerelease": "run-p compile documentation",
    "release": "npx semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbeesley/push-function-zips.git"
  },
  "author": "Bill Beesley <bill@beesley.dev>",
  "license": "MIT",
  "eslintConfig": {
    "parserOptions": {
      "project": [
        "./tsconfig.json",
        "./tsconfig-lint.json"
      ]
    }
  },
  "xo": {
    "space": true,
    "prettier": true,
    "rules": {
      "func-names": [
        "error",
        "always"
      ],
      "no-await-in-loop": "off",
      "@typescript-eslint/no-implicit-any-catch": "off",
      "unicorn/no-array-reduce": "off",
      "import/extensions": "off",
      "n/prefer-global/process": "off",
      "n/file-extension-in-import": "off",
      "unicorn/no-process-exit": "off"
    },
    "overrides": [
      {
        "files": "**/*.ts",
        "rules": {
          "@typescript-eslint/naming-convention": [
            "error",
            {
              "selector": "default",
              "format": [
                "camelCase"
              ]
            },
            {
              "selector": "variable",
              "format": [
                "camelCase",
                "UPPER_CASE",
                "PascalCase"
              ]
            },
            {
              "selector": "parameter",
              "format": [
                "camelCase",
                "PascalCase"
              ]
            },
            {
              "selector": "typeLike",
              "format": [
                "PascalCase"
              ]
            },
            {
              "selector": "classProperty",
              "format": [
                "camelCase"
              ]
            },
            {
              "selector": "objectLiteralProperty",
              "format": null
            },
            {
              "selector": "typeProperty",
              "format": null
            },
            {
              "selector": "enum",
              "format": [
                "PascalCase",
                "UPPER_CASE"
              ]
            },
            {
              "selector": "enumMember",
              "format": null
            }
          ]
        }
      },
      {
        "files": "**/*.test.ts",
        "rules": {
          "import/no-extraneous-dependencies": "off",
          "@typescript-eslint/naming-convention": "off"
        }
      }
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@beesley/tsconfig": "^3.0.0",
    "@commitlint/cli": "^19.0.1",
    "@commitlint/config-conventional": "^19.0.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^22.0.0",
    "@types/sinon": "^17.0.0",
    "@types/yargs": "^17.0.22",
    "ava": "^6.0.0",
    "aws-sdk-client-mock": "^4.0.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "cz-conventional-changelog": "^3.3.0",
    "documentation": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.0",
    "sinon": "^19.0.0",
    "typescript": "^5.0.4",
    "xo": "^0.59.3"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md",
            "README.md"
          ],
          "message": "chore(release): ${nextRelease.version} ðŸš€ [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.282.0",
    "@google-cloud/storage": "^7.0.0",
    "@jorgeferrero/stream-to-buffer": "^2.0.6",
    "better-logging": "^5.0.0",
    "globby": "^14.0.0",
    "jszip": "^3.10.1",
    "yargs": "^17.7.1"
  },
  "ava": {
    "files": [
      "src/*.test.js"
    ],
    "failFast": false,
    "failWithoutAssertions": false,
    "verbose": true,
    "timeout": "3m",
    "snapshotDir": "__snapshots__"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
